{
  "service": {
    "CorsService": {
      "_id": "",
      "_type": {
        "_id": "CorsService",
        "collection": false,
        "name": "CORS Service"
      },
      "enabled": true,
      "location": "global",
      "nextDescendents": [
        {
          "_id": "cors",
          "_type": {
            "_id": "configuration",
            "collection": true,
            "name": "Cors Configuration"
          },
          "acceptedHeaders": [
            "authorization",
            "x-openidm-username",
            "if-none-match",
            "x-openidm-nosession",
            "x-openidm-password",
            "accept-api-version",
            "x-requested-with",
            "content-type",
            "if-match",
            "cache-control"
          ],
          "acceptedMethods": [
            "HEAD",
            "DELETE",
            "POST",
            "GET",
            "PUT",
            "PATCH"
          ],
          "acceptedOrigins": [
            "http://localhost:8083",
            "http://localhost:8888",
            "http://localhost:8082"
          ],
          "allowCredentials": true,
          "enabled": true,
          "exposedHeaders": [
            "WWW-Authenticate"
          ],
          "maxAge": 600
        }
      ]
    }
  }
}
